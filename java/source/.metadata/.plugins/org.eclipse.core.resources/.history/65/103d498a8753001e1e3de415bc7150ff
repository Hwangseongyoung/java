package tut06;

public class OperEx {
	public static void main(String[] args) {
		int num1 = 5;
		int num2 = 10;
		
		String s1 = "koreait";
		String s2 = "Koreait";
		
//		비교
		System.out.println(num1 > num2); // true, false
		System.out.println(num1 == num2);
		System.out.println(s1.equals(s2)); // 문자열 동등 비교
		System.out.println(!s1.equals(s2));
		
//		타입에 저장
		
	}
}
