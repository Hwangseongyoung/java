package tut07_2;

public class HelloEx2 {
	
//	접근제한자 리턴타입 메소드명() {
//		
//	}
	public void title(String str) {
//		부산 Koreait 아카데미
//		서울 Koreait 아카데미
//		광주 Koreait 아카데미
		System.out.println(str+" Koreait 아카데미");
	}
	
	public static void main(String[] args) {
//		HelloEx2 클래스 안의 내용 전체를 가져온다.
		HelloEx2 busan = new HelloEx2();
		busan.title("부산");
		System.out.println("전화번호 : 051-123-4567");
		System.out.println("주소 : 부산광역시");
		busan.title("부산");
//		코리아아이티 아카데미 : syso
//		전화번호, 주소
//		코리아아이티 아카데미 : syso
		System.out.println("------------------------------");
		
		HelloEx2 seoul = new HelloEx2();
		seoul.title("서울");
	}
}
