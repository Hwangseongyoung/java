package tut06;

public class OperEx2 {
	public static void main(String[] args) {
		int std1 = 80;
		int std2 = 70;
		int avg = 75;
//		AND 이고, 이면서
//		OR 이거나, 또느
//		학생1 점수가 평균 점수보다 높고 학생2 점수가 평균보다 높으면
//		통과, 아니면 재평가
		
//		if(조건1 && 조건2) {
//			출력코드(참)
//		}
//		else {
//			출력코드(거짓)
//		}
		if(std1 >= avg && std2 >= avg) {
			System.out.println("통과");
		}
		else {
			System.out.println("재평가");
		}
	}
}
