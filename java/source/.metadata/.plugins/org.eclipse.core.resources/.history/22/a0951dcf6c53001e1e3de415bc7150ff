package tut06;

public class PrintEx {
	public static void main(String[] args) {
//		정수 10을 출력 : 10진수, 8진수, 16진수로 출력 -> printf();
//						d     o     x
		int num = 10;
//		출력값은 10입니다.
//		System.out.printf(출력예시, 변수);
		System.out.printf("출력값은 %d입니다.", num);
		System.out.println(String.format("출력값은 %d입니다.", num));
	}
}
