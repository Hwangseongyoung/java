package tut07_2;

public class HelloEx2 {
	
//	접근제한자 리턴타입 메소드명() {
//		
//	}
	public void title() {
		System.out.println("코리아아이티 아카데미");
	}
	
	public static void main(String[] args) {
//		HelloEx2 클래스 안의 내용 전체를 가져온다.
		HelloEx2 he = new HelloEx2();
		he.title();
		
		he.title();
//		코리아아이티 아카데미 : syso
//		전화번호, 주소
//		코리아아이티 아카데미 : syso
	}
}
